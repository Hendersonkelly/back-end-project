					<form action="/dashboard" method="POST">
						<div class="my-profile">
							<div class="heading">
								<h2>Profile Info</h2>
								<div class="heart-line">
									<i class="fas fa-heart"></i>
								</div>
							</div>

							<div id="dog-info" class="input-info-box">
								<div class="header">
									About Your Dog
								</div>
								<div class="content">
									<div class="row">
										<div class="col-md-6">
											<div class="my-input-box">
												<label>Dog Name</label>
												<input name="dogName" type="text" placeholder="Dog Name">
											</div>
										</div>
										<div class="col-md-6">
											<div class="my-input-box">
												<label>Zip Code</label>
												<input name="zipcode" type="integer" placeholder="Zip Code">
											</div>
										</div>
										<div class="col-md-6">
											<div class="my-input-box">
												<label>Breed</label>
												<input name="breed" type="text" placeholder="Breed">
											</div>
										</div>
										<div class="col-md-6">
											<div class="my-input-box">
												<label>Age</label>
												<input name="age" type="integer" placeholder="Age">
											</div>
										</div>
										<div class="col-md-6">
											<div id="fixed-div" class="my-input-box">
												<label>Is your dog fixed (spayed / neutered)?</label>
												<label for="yes">Yes
													<input name="fixed" value="yes" type="radio" placeholder="Yes"></label>
												<label for="no">No
													<input name="fixed" value="no" type="radio" placeholder="No"></label>
											</div>
										</div>
										<div class="col-md-6">
											<div id="energy-div" class="my-input-box">
												<label>Energy Level</label>
												<label for="couch-potato">Couch Potato
													<input name="energy" id="couch-potato" type="radio"
														placeholder="Couch Potato" value="Couch Potato"></label>
												<label for="frisky">Frisky
													<input name="energy" value="Frisky" id="frisky" type="radio"
														placeholder="Frisky"></label>
												<label for="ballistic">Insane
													<input name="energy" value="Insane" id="insane" type="radio"
														placeholder="Insane"></label>
											</div>
										</div>
										<div class="col-md-6">
											<div class="my-input-box" id="size-div">
												<label>Size</label>
												<label for="toy-teacup">Toy/Teacup
													<input name="size" value="Toy/Teacup" id="toy-teacup" type="radio"
														placeholder="Toy/Teacup"></label>
												<label for="small">Small
													<input name="size" value="Small" id="small" type="radio"
														placeholder="Small"></label>
												<label for="medium">Medium
													<input name="size" value="Medium" id="medium" type="radio"
														placeholder="Medium"></label>
												<label for="large">Large
													<input name="size" value="Large" id="large" type="radio"
														placeholder="Large"></label>
											</div>
										</div>
										<div class="col-md-6">
											<div class="my-input-box">
												<label>Description</label>
												<textarea name="description" id="description" cols="6" rows="4"
													placeholder="Describe your dog in a few sentences. Feel free to include links to their Instagram and/or TikTok pages!"></textarea>
											</div>
										</div>
									</div>
									<div class="input-info-box mt-5">
										<div class="header">
