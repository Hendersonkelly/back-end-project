														<label>Favorite Toys</label>
														<select name="faveToy" id="fave-toy">
															<option value="BALL IS LIFE">BALL IS LIFE</option>
															<option value="Stuffed Animal">Stuffed Animal</option>
															<option value="Chewy Toy / Bone">Chewy Toy / Bone</option>
															<option value="Owner's Shoes">Owner's Shoes</option>
															<option value="Anything that Squeaks">Anything that Squeaks
															</option>
														</select>
													</div>
													<div class="my-input-box">
														<label>Favorite Game</label>
														<select name="faveGame" id="fave-game">
															<option value="Tug-of-War">Tug-of-War</option>
															<option value="Chase">Chase</option>
															<option value="Wrestling">Wrestling</option>
															<option value="Bitey Face">Bitey Face</option>
															<option value="No Touch, Only Throw">No Touch, Only Throw
															</option>
														</select>
													</div>
												</div>
											</div>
										</div>
									</div>
								</div>
